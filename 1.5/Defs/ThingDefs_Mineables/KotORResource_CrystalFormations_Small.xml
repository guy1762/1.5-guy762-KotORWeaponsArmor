<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
	<ThingDef ParentName="KOTOR_CrystalFormation_Base">
		<defName>KOTOR_SmallCrystal_red</defName>
		<label>small crystal formation, red</label>
		<description>A small-sized crystal formation, warm to the touch. It radiates with Force energy.</description>
		<graphicData>
		  <texPath>Buildings/Crystal_Formations/small_dyeable</texPath>
		  <graphicClass>Graphic_Random</graphicClass>
		  <color>(200,65,90)</color>
		  <drawSize>(2.0,2.0)</drawSize>
		</graphicData>
		<statBases>
			<MaxHitPoints>500</MaxHitPoints>
			<Beauty>5</Beauty>
			<BeautyOutdoors>5</BeautyOutdoors>
			<MeditationFocusStrength>0.5</MeditationFocusStrength>
		</statBases>
		<building>
			<!--mineableThing>KotORChunk_durasteel</mineableThing>
			<mineableYield>1</mineableYield>
			<mineableScatterCommonality>0.25</mineableScatterCommonality>
			<mineableScatterLumpSizeRange>10~25</mineableScatterLumpSizeRange-->
		</building>
		<comps>
		  <li Class="CompProperties_Glower">
			<glowRadius>2</glowRadius>
			<glowColor>(200,65,90)</glowColor>
		  </li>
		</comps>
		<modExtensions>
		  <li Class="SecondaryMineableYield.ModExtension_SecondaryMineableYield">
			<mineableDropChance>1.0</mineableDropChance>
			<entries>
			  <li>
				<randomWeight>0.60</randomWeight>
				<mineableThing>guy762_crystalitem_red</mineableThing>
				<mineableYield>2</mineableYield>
			  </li>
			  <li>
				<randomWeight>0.40</randomWeight>
				<mineableThing>guy762_crystalitem_red</mineableThing>
				<mineableYield>1</mineableYield>
			  </li>

			</entries>
		  </li>
		</modExtensions>
	</ThingDef>
	
