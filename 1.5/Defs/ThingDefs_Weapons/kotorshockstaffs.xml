			<texPath>shockstaff</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>TransparentPostLight</shaderType>
			<drawSize>1.5</drawSize>
		</graphicData>
		<equippedAngleOffset>-30</equippedAngleOffset>
		<techLevel>Spacer</techLevel>
		<weaponClasses><li>MeleeBlunt</li></weaponClasses>
		<statBases>
			<MarketValue>1600</MarketValue>
			<Mass>7.5</Mass>
			
			<WorkToMake>16500</WorkToMake>
		</statBases>
		<equippedStatOffsets>
		  <MeleeHitChance>-0.2</MeleeHitChance>
		</equippedStatOffsets>
		
		<recipeMaker>
			<researchPrerequisite>guy762_ResearchKotOR_bluntmelee</researchPrerequisite>
			<skillRequirements>
				<Crafting>12</Crafting>
			</skillRequirements>
		</recipeMaker>
		<costList>
			<ComponentSpacer>6</ComponentSpacer>
			<Plasteel>50</Plasteel>
			<KOTOR_AlloyDurasteel>60</KOTOR_AlloyDurasteel>
		</costList>
		
