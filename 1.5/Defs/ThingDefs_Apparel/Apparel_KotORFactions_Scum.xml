			<Mass>4</Mass>
			<MarketValue>250</MarketValue>
			<MaxHitPoints>115</MaxHitPoints>
      		<ArmorRating_Sharp>0.4</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.45</ArmorRating_Blunt>
			<ArmorRating_Heat>0.5</ArmorRating_Heat>
