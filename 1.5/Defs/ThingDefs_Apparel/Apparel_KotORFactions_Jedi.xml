  <ThingDef ParentName="guy762_armorbase_soft">
    <defName>guy762_JediCloak_light</defName>
    <label>cloak, simple</label>
    <description>This is a simple robe worn by ordinary civilians and Jedi alike.</description>
    <graphicData>
      <texPath>Apparel/Jedi/cloak_1/Apparel</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
		<Mass>5</Mass>
		<MarketValue>50</MarketValue>
		<MaxHitPoints>100</MaxHitPoints>
		<ArmorRating_Sharp>0.01</ArmorRating_Sharp>
		<ArmorRating_Blunt>0.01</ArmorRating_Blunt>
		<ArmorRating_Heat>0.01</ArmorRating_Heat>
		<Insulation_Cold>25</Insulation_Cold>
        <Insulation_Heat>25</Insulation_Heat>
		<EquipDelay>3</EquipDelay>
		
		<WorkToMake>9500</WorkToMake>
	</statBases>
	<!--equippedStatOffsets>
	  <PsychicSensitivity>0.1</PsychicSensitivity>
      <PsychicEntropyRecoveryRate>0.1</PsychicEntropyRecoveryRate>
    </equippedStatOffsets-->
    <apparel>
		<drawData>
			<dataNorth>
			  <layer>79</layer>
			</dataNorth>
		</drawData>
		<layers>
			<li>Shell</li>
		</layers>
		<bodyPartGroups>
			<li>Torso</li>
			<li>Neck</li>
			<li>Shoulders</li>
			<li>Arms</li>
			<li>Legs</li>
		</bodyPartGroups>
		<tags Inherit="False">
			<li>KotORClothing_civilian_prole</li>
			<li>KotORClothing_civilian_hooded</li>
			<li>KotORClothing_simplecloak</li>
			<li>OR_jedi</li>
			<li>OR_jedicloak_light</li>
		</tags>
		<wornGraphicPath>Apparel/Jedi/cloak_1/Apparel</wornGraphicPath>
    </apparel>
	<tradeTags>
		<li>guy762TT_KotORClothes</li>
		<li>guy762TT_JediApparel</li>
	</tradeTags>
	<costList>
		<Synthread>80</Synthread>
	</costList>
	<recipeMaker>
		<skillRequirements>
			<Crafting>4</Crafting>
		</skillRequirements>
		<recipeUsers Inherit="False">
			<li>HandTailoringBench</li>
			<li>ElectricTailoringBench</li>
			<li>guy762_KotORWorkbench</li>
		</recipeUsers>
	</recipeMaker>
	<colorGenerator Class="ColorGenerator_Options" Inherit="False">
		<options>
			<li>
				<weight>95</weight>
				<only>(125,100,75)</only>       <!--K2 padawan-->
			</li>
			<li>
				<weight>100</weight>
				<only>(90,55,40)</only>         <!--K2 jedi knight-->
			</li>
		</options>
	</colorGenerator>
  </ThingDef>
  
