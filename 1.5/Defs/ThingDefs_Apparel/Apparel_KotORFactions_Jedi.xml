  <ThingDef ParentName="KotORArmorModdable_soft">
    <defName>guy762_JediTunic_light</defName>
    <label>Jedi tunic, light</label>
    <description>Members of the Jedi Order typically wear plain or unassuming garments.</description>
    <graphicData>
      <texPath>Apparel/Jedi/tunic_1_new/alt/Apparel</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <statBases>
		<Mass>3</Mass>
		<MarketValue>175</MarketValue>
		<MaxHitPoints>150</MaxHitPoints>
		<ArmorRating_Sharp>0.2</ArmorRating_Sharp>
		<ArmorRating_Blunt>0.2</ArmorRating_Blunt>
		<ArmorRating_Heat>0.2</ArmorRating_Heat>
		<Insulation_Cold>12.5</Insulation_Cold>
        <Insulation_Heat>12.5</Insulation_Heat>
		<EquipDelay>3</EquipDelay>
		
		<WorkToMake>10000</WorkToMake>
	</statBases>
	<equippedStatOffsets>
	  <MeleeDodgeChance>0.1</MeleeDodgeChance>
      <PsychicSensitivity>0.1</PsychicSensitivity>
    </equippedStatOffsets>
	
	<costList Inherit="False">
		<Synthread>80</Synthread>
		<!--VPE_Eltex>3</VPE_Eltex> added by patch -->
	  </costList>
	  <recipeMaker>
		<researchPrerequisite>guy762_ResearchKotOR_jedi</researchPrerequisite>
		<skillRequirements>
		  <Crafting>10</Crafting>
		</skillRequirements>
	  </recipeMaker>
	
    <apparel>
		<canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
		<layers>
			<li>OnSkin</li>
			<!--li>Middle</li-->
		</layers>
		<bodyPartGroups>
			<li>Torso</li>
			<!-- <li>Neck</li> -->
			<li>Shoulders</li>
			<li>Arms</li>
			<li>Hands</li>
			<li>Legs</li>
			<li>Feet</li>
		</bodyPartGroups>
		<tags Inherit="False">
			<li>OR_jedi</li>
			<li>OR_jeditunic_light</li>
			<li>KotORRoyaltyTag_AnyJediTunic</li>
		</tags>
		<wornGraphicPath>Apparel/Jedi/tunic_1_new/alt/Apparel</wornGraphicPath>
		<useWornGraphicMask>true</useWornGraphicMask>
    </apparel>
	<tradeTags>
		<li>guy762TT_JediApparel</li>
	</tradeTags>
	<colorGenerator Class="ColorGenerator_Options" Inherit="False">
		<options>
			<li>
				<weight>10</weight>
				<only>(195,75,35)</only>      <!-- new K1 red -->
			</li>
		</options>
	</colorGenerator>
	<comps>
	  <li Class="ModularWeapons.CompProperties_ModularParts">
		<compClass>ModularWeapons.CompModularParts</compClass>
		<partKinds>
		  <li>guy762_slot_underlay_robe</li>
		</partKinds>
		<randomizeParts>true</randomizeParts>
	  </li>
	</comps>
  </ThingDef>
  
  
